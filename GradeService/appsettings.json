{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Graylog"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Graylog",
        "Args": {
          "HostnameOrAddress": "localhost",
          "Port": 12201,
          "TransportType": "Udp"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Database=universystem-db;Username=user;Password=password"
  },
  "Kafka": {
    "Broker": "localhost:29091"
  },
  "Ports": {
    "Grpc": "5005",
    "Http": "5004"
  },
  "Graylog": {
    "Host": "localhost",
    "Port": 12201
  },
  "Jaeger": {
    "Host": "localhost",
    "Port": 6831
  }
}
